<?xml version="1.0" encoding="UTF-8"?>
<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    style="@style/activity_content_table_attrs" >
    <RelativeLayout style="@style/activity_content_table_relativelayout_attrs" >
        <TextView
            style="@style/activity_content_table_row_title_attrs"
            android:layout_width="match_parent"
            android:textColor="@color/black"
            android:gravity="center"
            android:text="@string/commodity_deliver_detail_group_no_title"/>
    </RelativeLayout>
    <View 
        style="@style/activity_content_divide_view_attrs" />
	 <RelativeLayout style="@style/activity_content_table_relativelayout_attrs" >
        <TextView
            android:id="@+id/commodity_deliver_detail_group_no"
            style="@style/activity_content_table_row_title_attrs"
            android:layout_width="match_parent"
            android:gravity="center"/>
    </RelativeLayout>

    <View 
        android:id="@+id/commodity_deliver_detail_sign_before_divide_view"
        style="@style/activity_content_divide_view_attrs" />
    <RelativeLayout 
        android:id="@+id/commodity_deliver_detail_sign_before_relativelayout"
        style="@style/activity_content_table_relativelayout_attrs" >
        <TextView
            style="@style/activity_content_table_row_title_attrs"
            android:text="@string/treatment_group_sign_image_title"
            android:layout_width="match_parent"
            android:textColor="@color/black"
            android:gravity="center"/>
    </RelativeLayout>
    <View 
        android:id="@+id/commodity_deliver_detail_sign_divide_view"
        style="@style/activity_content_divide_view_attrs" />

    <RelativeLayout
        android:id="@+id/commodity_deliver_detail_sign_relativelayout" 
        style="@style/activity_content_table_relativelayout_attrs" >
        <ImageView
            android:id="@+id/commodity_deliver_detail_sign_image"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />
    </RelativeLayout>

</TableLayout>
<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical" >
	<RelativeLayout
        android:id="@+id/treatment_select_employee_relativelayout"
        style="@style/activity_content_table_relativelayout_attrs">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="10dip"
            android:text="@string/select_employee"
            android:textSize="@dimen/text_size_16sp" />
    </RelativeLayout>
    <View
        android:layout_width="fill_parent"
        android:layout_height="1px"
        android:layout_marginTop="1dp"
        android:background="#CCCCCC" >
    </View>
    <RelativeLayout
        android:id="@+id/treatment_confirm_relativelayout"
        style="@style/activity_content_table_relativelayout_attrs" >
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="10dip"
            android:text="@string/treatment_confirm"
            android:textSize="@dimen/text_size_16sp" />
    </RelativeLayout>

    <View
        android:id="@+id/treatment_confirm_divide_view"
        android:layout_width="fill_parent"
        android:layout_height="1px"
        android:layout_marginTop="1dp"
        android:background="#CCCCCC" >
    </View>

    <RelativeLayout
        android:id="@+id/treatment_detail_relativelayout"
        style="@style/activity_content_table_relativelayout_attrs" >
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="10dip"
            android:text="@string/treatment_detail"
            android:textSize="@dimen/text_size_16sp" />
    </RelativeLayout>

    <View
        android:id="@+id/treatment_delete_divide_view"
        android:layout_width="fill_parent"
        android:layout_height="1px"
        android:layout_marginTop="1dp"
        android:background="#CCCCCC" >
    </View>

    <RelativeLayout
        android:id="@+id/treatment_delete_relativelayout"
        style="@style/activity_content_table_relativelayout_attrs" >
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="10dip"
            android:text="@string/treatment_delete"
            android:textSize="@dimen/text_size_16sp" />
    </RelativeLayout>
    <View
        android:layout_width="fill_parent"
        android:layout_height="1px"
        android:layout_marginTop="1dp"
        android:background="#CCCCCC" >
    </View>
    <RelativeLayout
        	android:id="@+id/treatment_designat_relativelayout"
       		style="@style/activity_content_table_relativelayout_attrs" >
        <TextView
            android:id="@+id/treatment_designat_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dip"
            android:layout_centerVertical="true"
            android:text="@string/treatment_designat"
            android:textSize="@dimen/text_size_16sp" />
    </RelativeLayout>
    <View
        android:layout_width="fill_parent"
        android:layout_height="1px"
        android:layout_marginTop="1dp"
        android:background="#CCCCCC" >
    </View>
</LinearLayout>
<!DOCTYPE HTML>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
    <meta name="viewport" content="initial-scale=1.0,maximum-scale=1.0,user-scalable=no" />
    <meta name="apple-mobile-web-app-capable" content="no" />
    <meta name="apple-touch-fullscreen" content="yes" />
    <meta name="format-detection" content="telephone=no" />
    <title>@ViewBag.Title</title>
    <link rel="stylesheet" type="text/css" href="/css/main.css" />
    <script src="/js/jquery-1.11.1.min.js"></script>
    <script src="/js/newIndex.js"></script>
    <script src="/js/goTop.js"></script>
</head>
<body>
    <div class="header bkgColor1 positionR"><a class="positionL headerLeftIcon" href="javascript:history.go(-1);">
        <img src="/pic/leftArrow.png" height="45"></a>@(ViewBag.Title)<a class="positionL headerRightIcon" href="/Home/Index"><img src="/pic/homeIcon.png" height="45"></a></div>
    @RenderBody()
    <div class="goTopSite">
        <ul class="tab">
            <li class="tabItem"><a class="tabItem bkgColor2 goTop" style="display: none;">
                <div class="imgArea MT10">
                    <img src="/pic/upArrow.png" height="50"></div>
            </a></li>
        </ul>
    </div>
</body>
</html>
